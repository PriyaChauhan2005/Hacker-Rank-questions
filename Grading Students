#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,n,r,f;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        if(a[i]>=38){
            r=a[i]/5;
            f=(5*(r+1)-a[i]);
                if(f<3){
                    a[i]=5*(r+1);
                }
            }
        }
     for(i=0;i<n;i++){
         cout<<a[i]<<endl;
     }   
}
